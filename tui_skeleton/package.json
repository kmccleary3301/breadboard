{
  "name": "breadboard-tui-skeleton",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "bin": {
    "breadboard": "dist/main.js"
  },
  "scripts": {
    "dev": "tsx src/main.ts",
    "build": "tsc -p tsconfig.json && tsx scripts/copyAsciiHeader.ts",
    "postbuild": "tsx scripts/installLocalBin.ts",
    "start": "node dist/main.js",
    "typecheck": "tsc --noEmit",
    "test": "vitest run",
    "runtime:gate:jitter": "node --import tsx scripts/runtime_jitter_gate.ts",
    "runtime:gate:jitter:matrix": "node --import tsx scripts/runtime_jitter_matrix.ts",
    "runtime:gate:noise": "node --import tsx scripts/runtime_transcript_noise_gate.ts",
    "runtime:gate:noise:warn": "node --import tsx scripts/runtime_noise_warn_ci.ts",
    "runtime:gate:noise:matrix": "node --import tsx scripts/runtime_noise_matrix.ts",
    "runtime:gate:subagents-scenarios": "node --import tsx scripts/subagents_scenario_gate.ts --strict",
    "runtime:gate:subagents-scenarios:warn": "node --import tsx scripts/subagents_scenario_gate.ts",
    "runtime:gate:focus-latency": "node --import tsx scripts/runtime_focus_latency_gate.ts --strict",
    "runtime:gate:focus-latency:warn": "node --import tsx scripts/runtime_focus_latency_gate.ts",
    "runtime:gate:strip-churn": "node --import tsx scripts/runtime_strip_churn_gate.ts --strict",
    "runtime:gate:strip-churn:warn": "node --import tsx scripts/runtime_strip_churn_gate.ts",
    "runtime:gate:dashboard": "node --import tsx scripts/runtime_gate_dashboard.ts",
    "runtime:triage:quick": "node --import tsx scripts/runtime_quick_triage_snapshots.ts",
    "runtime:gates:strict": "vitest run src/commands/repl/__tests__/runtimeTransitionGate.test.ts src/commands/repl/__tests__/runtimeSafetyGate.test.ts src/repl/markdown/__tests__/jitterGate.test.ts src/repl/markdown/__tests__/streamingStressGate.test.ts src/commands/repl/__tests__/transcriptNoiseGate.test.ts",
    "runtime:gates:p2": "bash scripts/runtime_p2_gate_pack.sh",
    "runtime:validate:ascii-no-color": "node --import tsx scripts/runtime_ascii_no_color_smoke.ts",
    "subagents:capture:cp0": "node --import tsx scripts/subagents_cp0_capture_scenarios.ts",
    "subagents:capture:cp1": "node --import tsx scripts/subagents_cp1_capture_scenarios.ts",
    "subagents:capture:cp2": "node --import tsx scripts/subagents_cp2_capture_scenarios.ts",
    "subagents:capture:cp3": "node --import tsx scripts/subagents_cp3_capture_scenarios.ts",
    "subagents:capture:cp3-cache": "node --import tsx scripts/subagents_cp3_cache_benchmark.ts",
    "subagents:capture:ex2": "node --import tsx scripts/subagents_ex2_tail_index_capture.ts",
    "subagents:capture:expansion": "node --import tsx scripts/subagents_expansion_capture_scenarios.ts",
    "subagents:bundle": "node --import tsx scripts/subagents_runtime_bundle.ts --strict",
    "subagents:bundle:warn": "node --import tsx scripts/subagents_runtime_bundle.ts",
    "subagents:validate:rollback": "node --import tsx scripts/subagents_rollback_validation.ts --strict",
    "subagents:validate:rollback:warn": "node --import tsx scripts/subagents_rollback_validation.ts",
    "subagents:closeout:metrics": "node --import tsx scripts/subagents_closeout_metrics.ts",
    "tui:goldens:report": "node --import tsx scripts/tui_goldens_report.ts",
    "tui:goldens:strict": "node --import tsx scripts/tui_goldens_strict.ts",
    "claude:compare:report": "node --import tsx scripts/claude_compare_report.ts"
  },
  "dependencies": {
    "@effect/cli": "^0.69.0",
    "@effect/cluster": "^0.47.0",
    "@effect/platform": "^0.90.2",
    "@effect/platform-node": "^0.95.0",
    "@effect/printer": "^0.45.0",
    "@effect/printer-ansi": "^0.45.0",
    "@stream-mdx/core": "^0.3.0",
    "@stream-mdx/worker": "^0.3.0",
    "@types/node": "^24.2.1",
    "chalk": "^5.3.0",
    "chroma-js": "^2.4.2",
    "clipboardy": "^4.0.0",
    "dotenv": "^16.4.5",
    "effect": "^3.17.7",
    "eventsource-parser": "^1.1.2",
    "graphemer": "^1.4.0",
    "ink": "^5.2.1",
    "ink-select-input": "^6.2.0",
    "ink-text-input": "^6.0.0",
    "react": "^18.2.0",
    "shiki": "^3.20.0",
    "yaml": "^2.8.1"
  },
  "optionalDependencies": {
    "keytar": "^7.9.0"
  },
  "devDependencies": {
    "@types/chroma-js": "^3.1.1",
    "@types/react": "^18.2.66",
    "ansi-to-svg": "^1.4.3",
    "ink-testing-library": "^4.0.0",
    "jpeg-js": "^0.4.4",
    "node-pty": "^1.0.0",
    "pngjs": "^7.0.0",
    "strip-ansi": "^7.1.0",
    "tsx": "^4.19.1",
    "typescript": "^5.5.4",
    "vitest": "^2.1.3"
  }
}
