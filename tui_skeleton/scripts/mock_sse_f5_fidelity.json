[
  {
    "delayMs": 0,
    "event": { "type": "stream.hello", "payload": { "version": "v2" } }
  },
  {
    "delayMs": 0,
    "event": { "type": "session.start", "payload": { "label": "f5 fidelity" } }
  },
  {
    "delayMs": 0,
    "event": { "type": "session.meta", "payload": { "model": "openai/gpt-5.1-mini-codex", "mode": "build" } }
  },
  {
    "delayMs": 0,
    "event": { "type": "run.start", "payload": { "reason": "mock" } }
  },
  {
    "delayMs": 50,
    "event": { "type": "assistant.reasoning.delta", "payload": { "delta": "Planning file update." } }
  },
  {
    "delayMs": 50,
    "event": {
      "type": "assistant.message.start",
      "payload": { "message_id": "m1", "channel": "text", "format": "markdown" }
    }
  },
  {
    "delayMs": 50,
    "event": {
      "type": "assistant.message.delta",
      "payload": { "message_id": "m1", "channel": "text", "delta": "Preparing to update notes.txt." }
    }
  },
  {
    "delayMs": 50,
    "event": {
      "type": "assistant.message.end",
      "payload": { "message_id": "m1", "channel": "text", "stop_reason": "tool_call" }
    }
  },
  {
    "delayMs": 50,
    "event": {
      "type": "assistant.tool_call.start",
      "payload": { "tool_call_id": "call_1", "tool_name": "write_file" }
    }
  },
  {
    "delayMs": 50,
    "event": {
      "type": "assistant.tool_call.delta",
      "payload": { "tool_call_id": "call_1", "args_text_delta": "{\"path\":\"notes.txt\",\"content\":\"Hello" }
    }
  },
  {
    "delayMs": 50,
    "event": {
      "type": "assistant.tool_call.delta",
      "payload": { "tool_call_id": "call_1", "args_text_delta": " world\"}" }
    }
  },
  {
    "delayMs": 50,
    "event": {
      "type": "assistant.tool_call.end",
      "payload": { "tool_call_id": "call_1", "args_text": "{\"path\":\"notes.txt\",\"content\":\"Hello world\"}" }
    }
  },
  {
    "delayMs": 50,
    "event": {
      "type": "permission.request",
      "payload": {
        "request_id": "perm_1",
        "kind": "file_write",
        "tool_call_id": "call_1",
        "title": "Write file",
        "description": "Allow write to notes.txt?",
        "action": { "tool_name": "write_file", "path": "notes.txt", "diff_summary": "\u0394 +1/-0 in notes.txt" },
        "options": [
          { "id": "allow_once", "label": "Allow once", "default": true },
          { "id": "deny", "label": "Deny" }
        ],
        "timeout_ms": 120000
      }
    }
  },
  {
    "delayMs": 5000,
    "event": {
      "type": "permission.decision",
      "payload": { "request_id": "perm_1", "decision": "allow_once", "decision_id": "dec_1" }
    }
  },
  {
    "delayMs": 50,
    "event": {
      "type": "tool.exec.start",
      "payload": { "tool_call_id": "call_1", "exec_id": "exec_1", "command": "write_file notes.txt", "tool_name": "write_file" }
    }
  },
  {
    "delayMs": 50,
    "event": {
      "type": "tool.exec.stdout.delta",
      "payload": { "tool_call_id": "call_1", "exec_id": "exec_1", "delta": "wrote 11 bytes\n" }
    }
  },
  {
    "delayMs": 50,
    "event": {
      "type": "tool.exec.stderr.delta",
      "payload": { "tool_call_id": "call_1", "exec_id": "exec_1", "delta": "warn: overwrote existing\n" }
    }
  },
  {
    "delayMs": 50,
    "event": {
      "type": "tool.exec.end",
      "payload": { "tool_call_id": "call_1", "exec_id": "exec_1", "exit_code": 0 }
    }
  },
  {
    "delayMs": 50,
    "event": {
      "type": "tool.result",
      "payload": { "tool_call_id": "call_1", "exec_id": "exec_1", "ok": true, "summary": "notes.txt updated" }
    }
  },
  {
    "delayMs": 50,
    "event": {
      "type": "warning",
      "payload": { "message": "Non-fatal warning while writing file." }
    }
  },
  {
    "delayMs": 50,
    "event": {
      "type": "assistant.thought_summary.delta",
      "payload": { "delta": "File write complete; ready to respond." }
    }
  },
  {
    "delayMs": 50,
    "event": {
      "type": "assistant.message.start",
      "payload": { "message_id": "m2", "channel": "text", "format": "markdown" }
    }
  },
  {
    "delayMs": 50,
    "event": {
      "type": "assistant.message.delta",
      "payload": { "message_id": "m2", "channel": "text", "delta": "notes.txt updated successfully." }
    }
  },
  {
    "delayMs": 50,
    "event": {
      "type": "assistant.message.end",
      "payload": { "message_id": "m2", "channel": "text", "stop_reason": "end_turn" }
    }
  },
  {
    "delayMs": 50,
    "event": { "type": "run.end", "payload": { "status": "ok" } }
  }
]
